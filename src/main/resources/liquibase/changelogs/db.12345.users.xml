<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="add users and authorities tables" author="redin_d" logicalFilePath="db.12345.users">
        <createTable tableName="users_test">
            <column name="id" autoIncrement="true" type="BIGINT">
                <constraints primaryKey="true"/>
            </column>
            <column name="first_name" type="VARCHAR">
                <constraints nullable="false"/>
            </column>
            <column name="last_name" type="VARCHAR">
                <constraints nullable="false"/>
            </column>
            <column name="email" type="VARCHAR"/>
            <column name="phone" type="BIGINT"/>
            <column name="status" type="BOOLEAN" defaultValueBoolean="true"/>
        </createTable>

        <sql>
            INSERT INTO users_test(last_name, first_name, phone, email)
            values('adm123in', 'lasta33dmin', 89119998888, 'adminadmin')
        </sql>
    </changeSet>

</databaseChangeLog>